var apagar = (function () {
	document.getElementsByClassName("_5pbj")[0].click();

	setTimeout(function() {
		document.getElementsByClassName("_54nc")[document.getElementsByClassName("_54nc").length - 2].click();

		setTimeout(function() {
			document.getElementsByName("ok")[0].click();
			document.getElementsByClassName("autofocus")[0].click();
		}, 500);
	}, 500);
});

setInterval(function() {
	apagar();
}, 2000);

